        MENUITEM "PointMesh",                   ID_POINTMESH
        MENUITEM "点扩展网格化",                      ID_POINTMESH2
    END
    POPUP "BallMesh"
    BEGIN
        MENUITEM "计算法矢和权重",                     ID_BALLMESH_NORMALANDWEIGHT
        MENUITEM "生成自适应球",                      ID_BALLMESH_GENERATORBALL
